{
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ],
    "[handlebars]": {
        "editor.formatOnSave": false
    },
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single"
}